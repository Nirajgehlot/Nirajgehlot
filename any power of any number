#include<iostream>
using namespace std;
int callPower(int num , int power)
{
	int answer=1;
	for(int i=1; i<=power; i++)
	{
		answer=answer*num;
	}
	cout<<num<<"^"<<power<<" = "<<answer;
}
int main()
{
	int num,power;
	cout<<"enter num = ";
	cin>>num;
	cout<<"enter power = ";
	cin>>power;
	callPower(num,power);
	return 0;
}
